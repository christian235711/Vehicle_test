            END IF;           
                        
 
                IF V_LIGNE != '' THEN res := MKT.MKTQAUT.F_WRITE_CVS_UTF8(file_id_cvs, ','); END IF;
V_LIGNE := '                        "earlyTerminationFlag":' || S_VEHICULE_ENDOFLIFE.EARLYTERMINATIONFLAG || '';res := MKT.MKTQAUT.F_WRITE_CVS_UTF8(file_id_cvs, V_LIGNE);
 


