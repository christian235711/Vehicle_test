S_mktqt_veh.nedcExtraUrbanFuelConsumption || '|' ||
S_mktqt_veh.nedcUrbanFuelConsumption || '|' ||
S_mktqt_veh.wltpLowFuelConsumption || '|' ||
S_mktqt_veh.wltpMediumFuelConsumption || '|' ||
S_mktqt_veh.wltpHighFuelConsumption || '|' ||
