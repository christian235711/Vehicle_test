	FUNCTION EXPORT_transco(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_transco Is 
		SELECT  
Entity			,
code			,
startValidityDate	,
endValidityDate		,
parent			,
parent_details_entity		,
parent_details_code		,
description_language		,
description_label		,
crossref_typecode		,
crossref_country		,
crossref_code			,
translation_language	,
translation_label	,
bomAttribut		,
countryScope	
        FROM MKTREF.MKTQT_transco ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_transco', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'Entity|code|startValidityDate|endValidityDate|parent|parent_details_entity|parent_details_code|description_language|description_label|crossref_typecode|crossref_country|crossref_code|translation_language|translation_label|bomAttribut|countryScope'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_transco IN C_mktqt_transco LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
                    
S_mktqt_transco.Entity			|| '|' ||
S_mktqt_transco.code			|| '|' ||
S_mktqt_transco.startValidityDate	|| '|' ||
S_mktqt_transco.endValidityDate		|| '|' ||
S_mktqt_transco.parent			|| '|' ||
S_mktqt_transco.parent_details_entity		|| '|' ||
S_mktqt_transco.parent_details_code		|| '|' ||
S_mktqt_transco.description_language		|| '|' ||
S_mktqt_transco.description_label		|| '|' ||
S_mktqt_transco.crossref_typecode		|| '|' ||
S_mktqt_transco.crossref_country		|| '|' ||
S_mktqt_transco.crossref_code			|| '|' ||
S_mktqt_transco.translation_language	|| '|' ||
S_mktqt_transco.translation_label	|| '|' ||
S_mktqt_transco.bomAttribut		|| '|' ||
S_mktqt_transco.countryScope	;
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_transco ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_transco', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_transco','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_transco;






	FUNCTION MAIN_ALIM_veh (NOMLOG VARCHAR2, P_DATE_TRAI DATE,  V_INSERTS   OUT NUMBER,
                                V_UPDATES   OUT NUMBER,
                                V_Error     Out Varchar2 ) RETURN NUMBER IS
		
		V_RET NUMBER := 0;                     /* erreur = 0 ou 1 */

	--INSERTS   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_UPDATES   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_ERROR   VARCHAR2(255);  			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/

		V_ERR   NUMBER := 0;      


        BEGIN	
     ---------------01			
      V_ERR     := ALIM_MODEL_GEN( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------02
      V_ERR     := ALIM_TRIM( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------03
      V_ERR     := ALIM_MOTORISATIONBLVD( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------04
      V_ERR     := ALIM_DIMENSIONS( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
		---------------05	
			V_ERR     := ALIM_veh( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;

		---------------06		
			V_ERR     := ALIM_veh_crossRefs( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
      
        ---------------07	--ON EST LA
			V_ERR     := ALIM_veh_pricings( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        -------------------08
  	V_ERR     := ALIM_veh_options( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
        
            
         -------------------09
			V_ERR     := ALIM_veh_optionslabels( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        -------------------10
			V_ERR     := ALIM_veh_optionspricings( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;            
            
         -------------------11
			V_ERR     := ALIM_veh_baseEquipments( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;          
 
            
			
			RETURN V_RET;

		/* AJOUTER APRES end; */
		
	END MAIN_ALIM_veh;




	FUNCTION MAIN_ALIM_transco (NOMLOG VARCHAR2, P_DATE_TRAI DATE,  V_INSERTS   OUT NUMBER,
                                V_UPDATES   OUT NUMBER,
                                V_Error     Out Varchar2 ) RETURN NUMBER IS
		
		V_RET NUMBER := 0;                     /* erreur = 0 ou 1 */

	--INSERTS   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_UPDATES   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_ERROR   VARCHAR2(255);  			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/

		V_ERR   NUMBER := 0;      


        BEGIN	

		---------------01			
			V_ERR     := ALIM_transco_model_gen( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;

		---------------02			
			V_ERR     := ALIM_transco_trim( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        ---------------03			
			V_ERR     := ALIM_transco_motorisationBLVD( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        -------------------04
			V_ERR     := ALIM_transco_dimensions( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
      -------------------05
			V_ERR     := UPDATE_transco_model( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
			
			RETURN V_RET;

		/* AJOUTER APRES end; */
		
	END MAIN_ALIM_transco;


	FUNCTION MAIN_ABSENT_code (NOMLOG VARCHAR2, P_DATE_TRAI DATE,  V_INSERTS   OUT NUMBER,
                                V_UPDATES   OUT NUMBER,
                                V_Error     Out Varchar2 ) RETURN NUMBER IS
		
		V_RET NUMBER := 0;                     /* erreur = 0 ou 1 */

	--INSERTS   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_UPDATES   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_ERROR   VARCHAR2(255);  			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/

		V_ERR   NUMBER := 0;      


        BEGIN	
     ---------------01			
      V_ERR     := ALIM_ABSENT_BLVD( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------02
      V_ERR     := ALIM_ABSENT_MODEL( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------03
      V_ERR     := ALIM_ABSENT_SEGMENT( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
    ---------------04
      V_ERR     := ALIM_ABSENT_transmission( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
		---------------05	
			V_ERR     := ALIM_ABSENT_TRIM( NOMLOG , P_DATE_TRAI , V_INSERTS , V_UPDATES , V_ERROR );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;

	
           
			
			RETURN V_RET;

		/* AJOUTER APRES end; */
		
	END MAIN_ABSENT_code;
  
  
    FUNCTION MAIN_EXPORT_veh(NOMLOG	VARCHAR2,
                        P_DATE_TRAI	DATE,
                        P_PATH    VARCHAR2,
                        P_FILENAME_VEH  VARCHAR2,
                        P_FILENAME_crossRefs  VARCHAR2,
                        P_FILENAME_pricings  VARCHAR2,
                        P_FILENAME_options VARCHAR2,
                        P_FILENAME_optionslabels  VARCHAR2,
                        P_FILENAME_optionspricings  VARCHAR2,
                        P_FILENAME_baseEquipments  VARCHAR2,
                        P_FILENAME_transco  VARCHAR2) Return Number is  

	--FUNCTION MAIN_EXPORT_veh (NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2 ) RETURN NUMBER IS
		
		V_RET NUMBER := 0;                     /* erreur = 0 ou 1 */

	--INSERTS   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_UPDATES   NUMBER := 0;			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/
	--	V_ERROR   VARCHAR2(255);  			/* DECLARATION DU PARAMETRE DE LA FONCTION D'ALIMENTAITON*/

		V_ERR   NUMBER := 0;      


        BEGIN	
     
		---------------05	
			V_ERR     := EXPORT_VEH( NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_VEH  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;

		---------------06		
			V_ERR     := EXPORT_crossRefs( NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_crossRefs  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
      
        ---------------07	
			V_ERR     := EXPORT_veh_pricings(  NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_pricings  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        -------------------08
        V_ERR     := EXPORT_veh_options(  NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_options  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
                 
         -------------------09
			V_ERR     := EXPORT_veh_optionslabels(  NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_optionslabels  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;
            
        -------------------10
			V_ERR     := EXPORT_veh_optionspricings(  NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_optionspricings  );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;            
            
         -------------------11
			V_ERR     := EXPORT_veh_baseEquipments( NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_baseEquipments );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;        

         -------------------12
			V_ERR     := EXPORT_transco( NOMLOG, P_DATE_TRAI, P_PATH, P_FILENAME_transco );  
			IF V_ERR = 1 THEN
				V_RET := 1;
				RETURN V_RET;
			END IF;    
            
			
			RETURN V_RET;

		/* AJOUTER APRES end; */
		
	END MAIN_EXPORT_veh;



END MKTQA_CAP_UK_TEST_BIS;
