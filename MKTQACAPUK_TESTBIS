					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_VEH ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_VEH', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_VEH','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_VEH;



	
	FUNCTION EXPORT_crossRefs(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_crossRefs Is 
		SELECT  
tech_id			as tech_id			,
itemDirectoryVehicleRef as itemDirectoryVehicleRef ,
versionYear		as versionYear		,
versionTrimester	as versionTrimester	,
VEHICULE_TYPE		as VEHICULE_TYPE		,
generation		as generation		,
DIMENSIONS		as DIMENSIONS		,
MODEL_GEN		as MODEL_GEN		,
BLVD		as BLVD		
  
        FROM MKTREF.MKTQT_veh_crossRefs
        order by tech_id
        ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_crossRefs', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'tech_id|itemDirectoryVehicleRef|versionYear|versionTrimester|VEHICULE_TYPE|generation|DIMENSIONS|MODEL_GEN|BLVD'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_crossRefs IN C_mktqt_crossRefs LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
                    
S_mktqt_crossRefs.tech_id			        || '|' || 
S_mktqt_crossRefs.itemDirectoryVehicleRef || '|' || 
S_mktqt_crossRefs.versionYear		    || '|' || 
S_mktqt_crossRefs.versionTrimester	|| '|' || 
S_mktqt_crossRefs.VEHICULE_TYPE		|| '|' || 
S_mktqt_crossRefs.generation		|| '|' || 
S_mktqt_crossRefs.DIMENSIONS		|| '|' || 
S_mktqt_crossRefs.MODEL_GEN		|| '|' || 
S_mktqt_crossRefs.BLVD		;
						
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_crossRefs ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_crossRefs', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_crossRefs','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_crossRefs;




	
	FUNCTION EXPORT_veh_pricings(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_veh_pricings Is 
		SELECT  
tech_id,    
itemDirectoryVehicleRef, 
versionYear,  
versionTrimester,
generation,
DIMENSIONS, 
MODEL_GEN,
VEHICULE_TYPE, 
tariffAreaCode,
priceCode,
priceType,
priceTypeCustomer,
priceStartDate,
priceEndDate,
vatAmount,
priceET,
priceIT,
priceIncludeVAT,
currency,
immAmount,
immRate,
novaAmount,
novaRate,
priceWithoutOtrCostIT,
priceWithoutOtrCostET,
priceWithoutOtrCostIncludeVAT

from MKTREF.MKTQT_veh_pricings ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_veh_pricings', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'tech_id|itemDirectoryVehicleRef|versionYear|versionTrimester|generation|DIMENSIONS|MODEL_GEN|VEHICULE_TYPE|tariffAreaCode|priceCode|priceType|priceTypeCustomer|priceStartDate|priceEndDate|vatAmount|priceET|priceIT|priceIncludeVAT|currency|immAmount|immRate|novaAmount|novaRate|priceWithoutOtrCostIT|priceWithoutOtrCostET|priceWithoutOtrCostIncludeVAT'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_veh_pricings IN C_mktqt_veh_pricings LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
S_mktqt_veh_pricings.tech_id       || '|' ||
S_mktqt_veh_pricings.itemDirectoryVehicleRef    || '|' ||
S_mktqt_veh_pricings.versionYear     || '|' ||
S_mktqt_veh_pricings.versionTrimester   || '|' ||
S_mktqt_veh_pricings.generation   || '|' ||
S_mktqt_veh_pricings.DIMENSIONS    || '|' ||
S_mktqt_veh_pricings.MODEL_GEN   || '|' ||
S_mktqt_veh_pricings.VEHICULE_TYPE    || '|' ||
S_mktqt_veh_pricings.tariffAreaCode   || '|' ||
S_mktqt_veh_pricings.priceCode   || '|' ||
S_mktqt_veh_pricings.priceType   || '|' ||
S_mktqt_veh_pricings.priceTypeCustomer   || '|' ||
S_mktqt_veh_pricings.priceStartDate   || '|' ||
S_mktqt_veh_pricings.priceEndDate   || '|' ||
S_mktqt_veh_pricings.vatAmount   || '|' ||
S_mktqt_veh_pricings.priceET   || '|' ||
S_mktqt_veh_pricings.priceIT   || '|' ||
S_mktqt_veh_pricings.priceIncludeVAT   || '|' ||
S_mktqt_veh_pricings.currency   || '|' ||
S_mktqt_veh_pricings.immAmount   || '|' ||
S_mktqt_veh_pricings.immRate   || '|' ||
S_mktqt_veh_pricings.novaAmount   || '|' ||
S_mktqt_veh_pricings.novaRate   || '|' ||
S_mktqt_veh_pricings.priceWithoutOtrCostIT   || '|' ||
S_mktqt_veh_pricings.priceWithoutOtrCostET   || '|' ||
S_mktqt_veh_pricings.priceWithoutOtrCostIncludeVAT  ;

						
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_pricings ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'v', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_veh_pricings','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_veh_pricings;





	
	FUNCTION EXPORT_veh_options(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_veh_options Is 
		SELECT  
tech_id,   
itemDirectoryVehicleRef,
versionYear, 
versionTrimester,  
generation,
DIMENSIONS,  
MODEL_GEN,
VEHICULE_TYPE,  
code,
constructorCode,
typeCode,
isPack,
isAgregated,
startDate,
endDate --,
--description, 
--longdescription, 
--effectivefrom, 
--effectiveto, 
--basic  
        FROM MKTREF.MKTQT_veh_options ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_veh_options', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'tech_id|itemDirectoryVehicleRef|versionYear|versionTrimester|generation|DIMENSIONS|MODEL_GEN|VEHICULE_TYPE|code|constructorCode|typeCode|isPack|isAgregated|startDate|endDate';   --|description|longdescription|effectivefrom|effectiveto|basic'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_veh_options IN C_mktqt_veh_options LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
                    
S_mktqt_veh_options.tech_id     || '|' ||
S_mktqt_veh_options.itemDirectoryVehicleRef  || '|' ||
S_mktqt_veh_options.versionYear   || '|' ||
S_mktqt_veh_options.versionTrimester    || '|' ||
S_mktqt_veh_options.generation  || '|' ||
S_mktqt_veh_options.DIMENSIONS    || '|' ||
S_mktqt_veh_options.MODEL_GEN  || '|' ||
S_mktqt_veh_options.VEHICULE_TYPE    || '|' ||
S_mktqt_veh_options.code  || '|' ||
S_mktqt_veh_options.constructorCode  || '|' ||
S_mktqt_veh_options.typeCode  || '|' ||
S_mktqt_veh_options.isPack  || '|' ||
S_mktqt_veh_options.isAgregated  || '|' ||
S_mktqt_veh_options.startDate  || '|' ||
S_mktqt_veh_options.endDate ;        -- || '|' ||
--S_mktqt_veh_options.description       || '|' ||
--S_mktqt_veh_options.longdescription   || '|' ||
--S_mktqt_veh_options.effectivefrom     || '|' ||
--S_mktqt_veh_options.effectiveto       || '|' ||
--S_mktqt_veh_options.basic  	;
						
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_options ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_options', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_veh_options','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_veh_options;



	
	FUNCTION EXPORT_veh_optionslabels(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_veh_optionslabels Is 
		SELECT  
tech_id,    
itemDirectoryVehicleRef,
versionYear, 
versionTrimester,  
generation,
DIMENSIONS,  
MODEL_GEN,
VEHICULE_TYPE,
code,
language,
labelShort,
labelLong
        FROM MKTREF.MKTQT_veh_optionslabels ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_veh_optionslabels', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'tech_id|itemDirectoryVehicleRef|versionYear|versionTrimester|generation|DIMENSIONS|MODEL_GEN|VEHICULE_TYPE|code|language|labelShort|labelLong'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_veh_optionslabels IN C_mktqt_veh_optionslabels LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
                    
S_mktqt_veh_optionslabels.tech_id     || '|' ||
S_mktqt_veh_optionslabels.itemDirectoryVehicleRef || '|' ||
S_mktqt_veh_optionslabels.versionYear  || '|' ||
S_mktqt_veh_optionslabels.versionTrimester   || '|' ||
S_mktqt_veh_optionslabels.generation || '|' ||
S_mktqt_veh_optionslabels.DIMENSIONS   || '|' ||
S_mktqt_veh_optionslabels.MODEL_GEN || '|' ||
S_mktqt_veh_optionslabels.VEHICULE_TYPE || '|' ||
S_mktqt_veh_optionslabels.code || '|' ||
S_mktqt_veh_optionslabels.language || '|' ||
S_mktqt_veh_optionslabels.labelShort || '|' ||
S_mktqt_veh_optionslabels.labelLong 	;
						
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_optionslabels ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_optionslabels', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_veh_optionslabels','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_veh_optionslabels;



	
	FUNCTION EXPORT_veh_optionspricings(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

		V_ERR                NUMBER := 0;	
		N_SAUV               NUMBER:=0;		
		v_ligne         VARCHAR2(4000);		
		file_id_cvs     utl_file.file_type;  
		file_name       VARCHAR2(30); 	 

		FILE_ID   UTL_FILE.FILE_TYPE; 
		RES       NUMBER := 0;			


		Cursor C_mktqt_veh_optionspricings Is 
		SELECT  
tech_id,   
itemDirectoryVehicleRef, 
versionYear, 
versionTrimester,
generation,
DIMENSIONS,  
MODEL_GEN,
VEHICULE_TYPE,
code,
territorialityCode,
pricebookCode,
pricebookType,
pricebookTypeCustomer,
pricebookStartDate,
pricebookEndDate,
vatAmount,
currency,
priceET,
priceIT,
priceIncludeVAT,
basePriceWithChargesET,
basePriceWithChargesIT,
basePriceWithChargesIncludeVAT
        FROM MKTREF.MKTQT_veh_optionspricings ;  


		BEGIN
			file_name := p_filename;
			FILE_ID := MKT.MKTQAUT.F_OPEN(NOMLOG);
			file_id_cvs:= MKT.MKTQAUT.F_OPEN_CVS(p_path,file_name);
			RES     := MKT.MKTQAUT.F_WRITE(FILE_ID, 'EXPORT_veh_optionspricings', ' ## MISE A DISPOSITION du fichier ##');

 			V_LIGNE := 'tech_id|itemDirectoryVehicleRef|versionYear|versionTrimester|generation|DIMENSIONS|MODEL_GEN|VEHICULE_TYPE|code|territorialityCode|pricebookCode|pricebookType|pricebookTypeCustomer|pricebookStartDate|pricebookEndDate|vatAmount|currency|priceET|priceIT|priceIncludeVAT|basePriceWithChargesET|basePriceWithChargesIT|basePriceWithChargesIncludeVAT'; 
 			
			RES := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, V_LIGNE);

			FOR S_mktqt_veh_optionspricings IN C_mktqt_veh_optionspricings LOOP 

				BEGIN
					IF V_ERR=1 THEN EXIT;
					END IF;
				
					V_LIGNE := 
                    
S_mktqt_veh_optionspricings.tech_id    || '|' ||
S_mktqt_veh_optionspricings.itemDirectoryVehicleRef  || '|' ||
S_mktqt_veh_optionspricings.versionYear  || '|' ||
S_mktqt_veh_optionspricings.versionTrimester || '|' ||
S_mktqt_veh_optionspricings.generation || '|' ||
S_mktqt_veh_optionspricings.DIMENSIONS   || '|' ||
S_mktqt_veh_optionspricings.MODEL_GEN || '|' ||
S_mktqt_veh_optionspricings.VEHICULE_TYPE || '|' ||
S_mktqt_veh_optionspricings.code || '|' ||
S_mktqt_veh_optionspricings.territorialityCode || '|' ||
S_mktqt_veh_optionspricings.pricebookCode || '|' ||
S_mktqt_veh_optionspricings.pricebookType || '|' ||
S_mktqt_veh_optionspricings.pricebookTypeCustomer || '|' ||
S_mktqt_veh_optionspricings.pricebookStartDate || '|' ||
S_mktqt_veh_optionspricings.pricebookEndDate || '|' ||
S_mktqt_veh_optionspricings.vatAmount || '|' ||
S_mktqt_veh_optionspricings.currency || '|' ||
S_mktqt_veh_optionspricings.priceET || '|' ||
S_mktqt_veh_optionspricings.priceIT || '|' ||
S_mktqt_veh_optionspricings.priceIncludeVAT || '|' ||
S_mktqt_veh_optionspricings.basePriceWithChargesET || '|' ||
S_mktqt_veh_optionspricings.basePriceWithChargesIT || '|' ||
S_mktqt_veh_optionspricings.basePriceWithChargesIncludeVAT  	;
						
											 

					res := MKT.MKTQAUT.F_WRITE_CVS(file_id_cvs, v_ligne);

					N_SAUV := N_SAUV + 1;
				EXCEPTION
					WHEN OTHERS THEN
						COMMIT;
						V_ERR  := 1;
						res     := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_optionspricings ', 'Message Erreur pl/sql : ' || SQLERRM );
					RETURN V_ERR;

				END;
			END LOOP;

			
			UTL_FILE.FCLOSE(file_id_cvs);
			res := MKT.MKTQAUT.F_WRITE(file_id, 'EXPORT_veh_optionspricings', 'Nombre de lignes Ecrites :' || N_SAUV);
			UTL_FILE.FCLOSE(file_id);


			RETURN V_ERR;

		EXCEPTION 
			WHEN OTHERS THEN
				COMMIT;
				V_ERR  := 1;
				res     := MKT.MKTQAUT.F_WRITE(file_id,'EXPORT_veh_optionspricings','Message Erreur pl/sql : ' || SQLERRM);
			RETURN V_ERR; 

		/*end;  */

	END EXPORT_veh_optionspricings;




	
	FUNCTION EXPORT_veh_baseEquipments(NOMLOG	VARCHAR2, P_DATE_TRAI	DATE, P_PATH    VARCHAR2, P_FILENAME  VARCHAR2) RETURN NUMBER AS

