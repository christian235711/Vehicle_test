    EXCEPTION WHEN OTHERS THEN
              V_NB_ID := NULL;
    END;

    IF NVL(V_NB_ID, 0) > 0 THEN
       V_LIGNE := ',' ; res := MKT.MKTQAUT.F_WRITE_CVS_UTF8(file_id_cvs, V_LIGNE); --------- virgule ajoutee
    
